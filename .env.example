# Medical Assistant Environment Variables
# Copy this file to .env and fill in your API keys

# Environment (development, production, testing)
MEDICAL_ASSISTANT_ENV=production

# LLM API Keys
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here

# STT (Speech-to-Text) API Keys
GROQ_API_KEY=your_groq_api_key_here
DEEPGRAM_API_KEY=your_deepgram_api_key_here
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# Optional: Ollama settings (if using local Ollama)
OLLAMA_HOST=http://localhost:11434

# Optional: Override default storage folder
# MEDICAL_ASSISTANT_STORAGE=/path/to/storage

# N8N RAG Integration
# N8N webhook URL for RAG document search
N8N_URL=your_n8n_webhook_url_here
# Authorization header for N8N webhook
N8N_AUTHORIZATION_SECRET=your_authorization_header_here